aws_profile         = "with-context"
aws_region          = "us-east-1"
instance_type       = "t3.medium"
instance_name       = "phone-coding"
ssh_public_key_path = "~/.ssh/id_ed25519.pub"
volume_size         = 30
use_spot            = false

